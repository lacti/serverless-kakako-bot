{
  "compilerOptions": {
    "sourceMap": true,
    "lib": ["es5", "es6", "es2015", "es2016", "es2017", "es2018"],
    "forceConsistentCasingInFileNames": true,
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "noImplicitAny": true,
    "strictNullChecks": true,
    "suppressImplicitAnyIndexErrors": true,
    "noUnusedLocals": true
  }
}
